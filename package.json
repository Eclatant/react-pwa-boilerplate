{
  "name": "bare-react-pwa",
  "version": "0.1.0",
  "engines": {
    "node": ">=6.0"
  },
  "dependencies": {
    "history": "2.1.1",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-mdl": "^1.7.2",
    "react-router": "^2.4.0",
    "scroll-behavior": "^0.5.0"
  },
  "scripts": {
    "start": "NODE_ENV=development node ./scripts/start.js",
    "build": "rm -rf build && NODE_ENV=production node ./scripts/build.js",
    "deploy": "gh-pages -d build",
    "test": "xo && NODE_ENV=test ava --verbose --serial",
    "lint": "xo"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-react-app": "^0.2.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.5.1",
    "eslint-config-xo": "^0.17.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.6.0",
    "file-loader": "0.9.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^9.8.3",
    "json-loader": "0.5.4",
    "match-media": "^0.2.0",
    "minimist": "^1.2.0",
    "postcss-loader": "0.13.0",
    "react-addons-test-utils": "^15.3.2",
    "style-loader": "0.13.1",
    "sw-precache": "^4.2.1",
    "sw-precache-webpack-plugin": "^0.5.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9",
    "xo": "^0.17.0"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "extends": [
      "xo",
      "xo-react"
    ],
    "rules": {
      "quote-props": [
        "error",
        "as-needed"
      ],
      "react/require-optimization": 0,
      "react/forbid-component-props": 0,
      "import/no-unresolved": 0,
      "import/no-unassigned-import": 0,
      "unicorn/filename-case": 0
    }
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
